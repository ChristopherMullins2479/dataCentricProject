<table border="1">
    <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Credits</th>
    <th>Update</th>
    <th>List Students</th>
    </tr>
    <% modules.forEach((module) =>
    { %>
    <tr>
    <td><%= module.mid %></td>
    <td><%= module.name %></td>
    <td><%= module.credits %></td>
    <td><button onclick="window.location.href='http://localhost:3004/update/<%= module.mid %>'">Update</button></td>
    <td><button onclick="window.location.href='http://localhost:3004/listStudentsInModule/<%= module.mid %>'">List Students</button></td>
    </tr>
    <% }) %>
    </table>
    <a href = http://localhost:3004/>Home</a>
    